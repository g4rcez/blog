{
  "name": "ultimate-blog",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "codemirror": "^5.58.1",
    "linq-arrays": "1.5.0",
    "marked": "^1.1.1",
    "ramda": "^0.27.1",
    "react": "^16.13.1",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "storage-manager-js": "4.0.4",
    "the-mask-input": "^2.1.5",
    "use-typed-reducer": "^2.0.1"
  },
  "scripts": {
    "posts": "node scripts/build.js",
    "css": "postcss src/styles/css/index.css -o src/styles/css/dist/index.css",
    "watch:css": "postcss src/styles/css/index.css -o src/styles/css/dist/index.css --watch",
    "start": "PORT=4000 react-scripts start",
    "build": "GENERATE_SOURCEMAP=false NODE_ENV=production yarn css && yarn posts && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/classnames": "^2.2.10",
    "@types/highlight.js": "^9.12.4",
    "@types/jest": "^24.0.0",
    "@types/marked": "^1.1.0",
    "@types/node": "^12.0.0",
    "@types/ramda": "^0.27.18",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "cssnano": "^4.1.10",
    "postcss-cli": "^8.0.0",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^5.0.0",
    "prettier": "^2.1.2",
    "react-scripts": "3.4.3",
    "tailwindcss": "^1.8.10",
    "typescript": "3.9.7"
  }
}
