{
    "compilerOptions": {
        "downlevelIteration": true,
        "esModuleInterop": true,
        "incremental": true,
        "isolatedModules": true,
        "module": "CommonJS",
        "moduleResolution": "node",
        "noEmit": true,
        "resolveJsonModule": true,
        "rootDir": "./open-graph-bot",
        "skipLibCheck": true,
        "strict": true,
        "target": "es5",
        "lib": [
            "DOM",
            "ESNext"
        ]
    },
    "exclude": [
        "node_modules"
    ]
}
